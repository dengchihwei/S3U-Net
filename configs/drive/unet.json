{
  "name": "DRIVE_LP_20",
  "data": {
    "type": "DriveDataset",
    "batch_size": 10,
    "args": {
      "data_dir": "../../VesselLearning/Datasets/DRIVE/",
      "image_sizes": null,
      "train": true,
      "patch_sizes": [256, 256],
      "spacings": [192, 192],
      "augment": true,
      "reverse": true,
      "single_ch": true
    }
  },
  "arch": {
    "type": "UNet2D",
    "supervised": true,
    "args": {
      "in_ch": 1,
      "out_ch": 1,
      "feat_dims": [64, 128, 256, 512]
    }
  },
  "optimizer": {
    "type": "Adam",
    "learning_rate": 0.001,
    "weight_decay": 1e-5,
    "amsgrad": true
  },
  "lr_scheduler": {
    "type": "StepLR",
    "step_size": 50,
    "gamma": 0.9
  },
  "trainer": {
    "train_type": "SUPERVISED",
    "linear_probe": true,
    "gpu_device_num": 1,
    "epoch_num": 2000,
    "save_period": 10,
    "resume": true,
    "resume_path": "../trained_models/DRIVE/DRIVE_ADAPTIVE_LC/2024-08-08-16/ADAPTIVE_LC-2000-epoch.pth",
    "checkpoint_dir": "../trained_models/DRIVE"
  }
}
