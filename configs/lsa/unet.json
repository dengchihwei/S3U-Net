{
  "name": "LSA_LP_20",
  "data": {
    "type": "LSADataset",
    "batch_size": 16,
    "args": {
      "data_dir": "../../VesselLearning/Datasets/DarkVessels/UnilateralData",
      "patch_sizes": [48, 48, 48],
      "spacings": [40, 40, 40],
      "train": true
    }
  },
  "arch": {
    "type": "UNet3D",
    "supervised": true,
    "args": {
      "in_ch": 1,
      "out_ch": 1,
      "feat_dims": [64, 128, 256, 512]
    }
  },
  "optimizer": {
    "type": "Adam",
    "learning_rate": 0.001,
    "weight_decay": 1e-5,
    "amsgrad": true
  },
  "lr_scheduler": {
    "type": "StepLR",
    "step_size": 50,
    "gamma": 0.9
  },
  "trainer": {
    "train_type": "SUPERVISED",
    "linear_probe": true,
    "gpu_device_num": 1,
    "epoch_num": 200,
    "save_period": 1,
    "resume": true,
    "resume_path": "../trained_models/LSA/LSA_ADAPTIVE_LC/2024-08-05-10/ADAPTIVE_LC-200-epoch.pth",
    "checkpoint_dir": "../trained_models/LSA"
  }
}
